{
	"compilerOptions": {
	  "module": "commonjs",
	  "target": "es2020",
	  "outDir": "out",
	  "lib": [
		"es2020",
		"dom"
	  ],
	  "sourceMap": true,
	  "rootDir": ".",
	  "strict": true,
	  "esModuleInterop": true,
